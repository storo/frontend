<form role="form">
  <h1>Login</h1>
  <div class="form-group">
    <label for="exampleInputEmail1">Email</label>
    {{view Ember.TextField valueBinding='email' class='form-control' name='email' placeholder='Email'}}
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    {{view Ember.TextField type="password" valueBinding='password' class='form-control' placeholder='Password'}}
  </div>
  <button type="submit" {{action save}} class="btn btn-default">Submit</button>
</form>
